class Example23
{
	public static void main(String[]args)
	{
		for (int i =0;i<=100;i++ )
		{
			System.out.println(i+ " -> It is the sleep after 4 sec");
		}
		try
		{
			Thread.sleep(5000);
		}
		catch (InterruptedException s)
		{
			//System.out.println("Before sleep the main method ");
		}
		
	}
}
// It is the throws the compile time exceptions -> InterruptedException
// unreported exception InterruptedException; must be caught or declared to be thrown Thread.sleep(4000);